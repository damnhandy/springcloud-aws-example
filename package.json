{
  "name": "springboot-app",
  "version": "0.1.0",
  "bin": {
    "infrastructure": "bin/springboot-app.js"
  },
  "scripts": {
    "build": "tsc && npm test",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "prepare": "husky install",
    "prebuild": "cd data-migration/ && zip -r ../data-migration.zip . && mv ../data-migration.zip ../data-migration-out && cd -",
    "postbuild": "cdk synth",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --write .",
    "prettier-watch": "onchange '**/*.*' -- prettier --write {{changed}}"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.113.0",
    "@prettier/plugin-ruby": "^1.6.0",
    "@types/aws-lambda": "^8.10.79",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "aws-cdk": "^1.113.0",
    "aws-lambda": "^1.0.6",
    "esbuild": "^0.12.15",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.4.2",
    "lint-staged": "^11.0.0",
    "onchange": "^7.1.0",
    "prettier": "2.3.1",
    "prettier-plugin-java": "^1.3.0",
    "prettier-plugin-sh": "^0.7.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~4.3.5"
  },
  "dependencies": {
    "@aws-cdk/aws-codebuild": "^1.113.0",
    "@aws-cdk/aws-ec2": "^1.113.0",
    "@aws-cdk/aws-ecr": "^1.113.0",
    "@aws-cdk/aws-ecr-assets": "^1.113.0",
    "@aws-cdk/aws-ecs": "^1.113.0",
    "@aws-cdk/aws-ecs-patterns": "^1.114.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.113.0",
    "@aws-cdk/aws-iam": "^1.113.0",
    "@aws-cdk/aws-kms": "^1.113.0",
    "@aws-cdk/aws-lambda": "^1.113.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.113.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.114.0",
    "@aws-cdk/aws-logs": "^1.113.0",
    "@aws-cdk/aws-rds": "^1.113.0",
    "@aws-cdk/aws-s3-assets": "^1.113.0",
    "@aws-cdk/aws-s3-deployment": "^1.113.0",
    "@aws-cdk/aws-s3-notifications": "^1.114.0",
    "@aws-cdk/aws-secretsmanager": "^1.113.0",
    "@aws-cdk/aws-ssm": "^1.113.0",
    "@aws-cdk/core": "^1.113.0",
    "@aws-sdk/client-codebuild": "^3.22.0",
    "@aws-sdk/client-s3": "^3.22.0",
    "@aws-sdk/client-ssm": "^3.22.0",
    "@aws-sdk/types": "^3.22.0",
    "cdk-ecr-deployment": "^0.0.54",
    "source-map-support": "^0.5.16"
  },
  "lint-staged": {
    "*.{js,css,md,java,sh}": "prettier --write"
  }
}
